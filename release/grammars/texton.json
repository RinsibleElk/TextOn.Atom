{
    "name": "texton",
    "scopeName": "source.texton",
    "fileTypes": [
        "texton"
    ],
    "foldingStartMarker": "",
    "foldingStopMarker": "",
    "patterns": [
        {
            "name": "comment.texton",
            "match": "^//.*$"
        },
        {
            "name": "keyword.var.texton",
            "match": "^@var\\s+"
        },
        {
            "name": "keyword.att.texton",
            "match": "^@att\\s+"
        },
        {
            "name": "keyword.include.texton",
            "match": "^#include\\s+"
        },
        {
          "name": "keyword.break.texton",
          "match": "^\\s*@break\\s*$"
        },
        {
          "name": "variable.parameter.texton",
          "match": "\\$[A-Za-z][A-Za-z0-9_]*"
        },
        {
          "name": "entity.other.attribute-name.texton",
          "match": "\\%[A-Za-z][A-Za-z0-9_]*"
        },
        {
            "name": "string.quoted.double.texton",
            "begin": "(?=[^\\\\])(\")",
            "end": "(\")",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.definition.string.begin.texton"
                }
            },
            "endCaptures": {
                "1": {
                    "name": "punctuation.definition.string.end.texton"
                }
            },
            "patterns": [
                {
                    "name": "punctuation.separator.string.ignore-eol.texton",
                    "match": "\\\\$[ \\t]*"
                },
                {
                    "name": "constant.character.string.escape.texton",
                    "match": "\\\\([\\\\''ntbr]|u[a-fA-F0-9]{4}|u[a-fA-F0-9]{8})"
                },
                {
                    "name": "invalid.illegal.character.string.texton",
                    "match": "\\\\(?![\\\\''ntbr]|u[a-fA-F0-9]{4}|u[a-fA-F0-9]{8})."
                }
            ]
        },
        {
          "name": "keyword.func.texton",
          "match": "^@func\\s+"
        },
        {
          "name": "keyword.seq.texton",
          "match": "^\\s*@seq"
        },
        {
          "name": "keyword.choice.texton",
          "match": "^\\s*@choice"
        },
        {
          "name": "support.function.texton",
          "match": "\\s*@[A-Za-z][A-Za-z0-9_]*\\(\\)"
        },
        {
          "name": "keyword.choice.operator.texton",
          "match": "(\\{|\\}|\\||&&)"
        }
    ],
    "repository": {
    }
}
